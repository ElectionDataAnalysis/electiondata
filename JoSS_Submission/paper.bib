@report{wack_election_2016,
	title = {Election Results Common Data Format Specification},
	url = {https://nvlpubs.nist.gov/nistpubs/SpecialPublications/NIST.SP.1500-100.pdf},
	abstract = {This publication describes an election results common data format specification for pre-election setup information and post-election results reporting. It contains a {UML} model of the election data and an {XML} format derived from the {UML} model. It also contains background information regarding how geopolitical geography is structured and used in the model and schema. The {XML} format is comprehensive and at the same time very flexible, able to accommodate election scenarios used throughout the U.S. It is part of a series of planned common data format specifications for voting equipment.},
	pages = {NIST SP 1500--100},
	number = {{NIST} {SP} 1500-100},
	institution = {National Institute of Standards and Technology},
	author = {{Wack}, John P. and Brace, Kim and Dana, Sam and Deutsch, Herb and Dziurlaj, John and Piper, Ian and Rehill, Don and Rivello, Richard and Whitt, Sarah},
	urldate = {2021-08-13},
	date = {2016-02},
	langid = {english},
	doi = {10.6028/NIST.SP.1500-100},
}

@report{wack_election_2019,
	location = {Gaithersburg, {MD}},
	title = {Election results common data format specification:: revision 2.0},
	url = {https://nvlpubs.nist.gov/nistpubs/SpecialPublications/NIST.SP.1500-100r2.pdf},
	shorttitle = {Election results common data format specification},
	abstract = {This publication describes an election results common data format specification for pre-election setup information and post-election results reporting. It contains a {UML} (Unified Modeling Language) model of the election data and an {XML} ({eXtensible} Markup Language) and {JSON} ({JavaScript} Object Notation) format derived from the {UML} model. It also contains background information regarding how geopolitical geography is structured and used in the model and schema. The {XML} format is comprehensive and at the same time very flexible, able to accommodate election scenarios used throughout the U.S. It is part of a series of planned common data format specifications for voting equipment.},
	pages = {NIST SP 1500--100r2},
	number = {{NIST} {SP} 1500-100r2},
	institution = {National Institute of Standards and Technology},
	author = {{Wack}, John},
	urldate = {2021-08-13},
	date = {2019-12},
	langid = {english},
	doi = {10.6028/NIST.SP.1500-100r2},
}
